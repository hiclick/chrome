{
    "author": "Christen",
	"manifest_version": 2,
	"name": "广告助手",
	"version": "1.1",
	"description": "广告助手",
	"icons": {
		"16": "images/icon_16.png",
		"48": "images/icon_48.png",
		"128": "images/icon_128.png"
	},

	"content_scripts": [
		{
			"matches": [
				"*://f.cn:*/adpubb/adTrace/adTraceCountReport.htm*"
			],
            "css": [
                "js/jquery-ui-192/css/custom-theme/jquery-ui-1.9.2.css",
                "css/main.css"
            ],
			"js": [
                "js/mootools/mootools-1.5.0.js",
                "js/jquery-ui-192/js/jquery-1.8.3.js",
                "js/jquery-ui-192/js/jquery-ui-1.9.2.min.js",
                "js/main.js"
			]
		}
	],
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs"
	]
}